/* This file builds a debuggable tree */


/* build only the cose bits */
#ifndef BUILD_UNITY_1_ONLY  
#define BUILD_UNITY_1_ONLY YES
#endif

/* build archived libraries in this tree */
#ifndef ForceNormalLib
#define ForceNormalLib YES
#endif

/* do not build shared libraries in this tree */
#ifndef HasSharedLibraries
#define HasSharedLibraries NO
#endif

/* debuggable tree */
#ifndef OptimizedCDebugFlags
#define OptimizedCDebugFlags    -g -D__STR31__
#endif

#define BUILD_DEBUGGABLE_CLIENTS /* build some clients debuggable */
#define DEBUGTREE
