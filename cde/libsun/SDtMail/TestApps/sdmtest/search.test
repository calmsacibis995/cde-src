for TRANSFERDIR in $STD_DIRS
do
    cd $TRANSFERDIR
    set `df -a . | tail -1`
    if [ $# -eq 6 ]
    then
        shift
    fi
    KB_AVAIL=$3
    BYTES_AVAIL=`expr $KB_AVAIL \* 1000`
    if [ $BYTES_AVAIL -ge  $BYTES_NEEDED ]
    then
        SPACE_FOUND=1
        break
    fi
