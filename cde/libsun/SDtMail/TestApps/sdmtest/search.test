#!/bin/sh
#
# This is a script intended to be mailed to all in USA in order
# to facilitate starting regis from their desktop.
#
# It assumes that regis will be installed in the default location of
# softdist, wherever that is.
#
# It also attempts to start a shell tool in order to record problems.
# if shelltool is not on the path, we are hosed.
#
# Author: Alan Nichols (SunU Technology)	Date this version: 8/3/94
#

if [ "$DISPLAY" != "" -a "$MYOWNWINDOW" = "" ]; then
	exec env MYOWNWINDOW=true ${OPENWINHOME:-/usr/openwin}/bin/shelltool -Wl "Regis shell" -WL "Regis" $0 &
	exit 1
fi

export DIST_HOME 

DIST_HOME=${UDHOME:-$DIST_HOME}
DIST_HOME=${DIST_HOME:-/usr/dist}

echo "Looking for Regis in your environment ... "

if [ -x $DIST_HOME/exe/regis -a -d $DIST_HOME/pkgs/regis ]
then
	$DIST_HOME/exe/regis
	sleep 20
	echo "Regis started."
else
	echo "Not Found."
	echo "REGIS is currently a U.S. application, distributed through softdist." 
	echo "Please contact your system administrator for assistance in starting REGIS."	
fi

echo "Hit return to continue ..."
read xyzzy

exit 0

