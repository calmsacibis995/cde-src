     (interactive "sBuffer name: ")
     (switch-to-buffer name)
     (end-of-buffer)
     (delete-region 1 (point)))

   (defun print-bug(id)
     "Prints bug ID in *id* buffer name"
     (interactive "sBug id: ")
     (bugtraq-clear-buffer (concat "*" id "*"))
     (message "printing bug %s" id)
