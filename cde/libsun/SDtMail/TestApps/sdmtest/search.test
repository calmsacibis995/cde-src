# My constized version
size -F libXm.so.3.rmg
1518050(r-x) + 73824(rwx) = 1591874

size -f libXm.so.3.rmg
15268(.hash) + 30624(.dynsym) + 37345(.dynstr) + 24(.rela.ex_shared) + 14040(.rela.data) + 61212(.rela.picdata) + 20832(.rela.got) + 12900(.rela.plt) + 1242844(.text) + 56(.init) + 56(.fini) + 4(.exception_ranges) + 57130(.rodata) + 25558(.rodata1) + 6948(.got) + 152(.dynamic) + 12952(.plt) + 32(.ex_shared) + 12472(.data) + 34796(.picdata) + 6468(.bss) = 1591713

