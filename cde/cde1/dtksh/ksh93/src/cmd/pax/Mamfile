exec - ?*:*:*|*::*|*:*:$RANDOM)
exec - 	;;
exec - *)	if	ENV= x= $SHELL -n -c '${list[level]} ' 2>/dev/null
exec - 	then	ENV= $SHELL -n bax.sh
exec - 	fi
exec - 	;;
exec - esac
exec - case '' in
exec - "")	cp bax.sh bax
exec - 	;;
exec - *)	{
exec - 	i=`(read x; echo $x) < bax.sh`
exec - 	case $i in
exec - 	'#!'*|*'||'*|':'*|'":"'*|"':'"*)	echo $i ;;
exec - 	esac
exec - 	echo 
exec - 	cat bax.sh
exec - 	} > bax
exec - 	;;
exec - esac
exec - chmod u+w,+x bax
done bax generated
