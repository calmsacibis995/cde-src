make ${INSTALLROOT}/include/ast/prototyped.h
make prototyped.h
exec - proto -f /dev/null > h.${COTEMP}.h
exec - if	cmp -s h.${COTEMP}.h prototyped.h
exec - then	rm -f h.${COTEMP}.h
exec - else	mv h.${COTEMP}.h prototyped.h
exec - fi
done prototyped.h generated
