<s1 id=CMenus>Calendar Menus

<procedure>&SubtopicsHeading;
<list bullet>
* <xref CEditMenu>
* <xref CViewMenu>
* <xref CScheduleMenu>
* <xref CBrowseMenu>
* <xref CPrintMenu>
<!-- Remove Print Menu and create File Menu when UI changes -->
<\list>


	<location id=CalendarMenuBar>&empty;<\location>

<!-- MODULE CEditMenu -->
<s2 id=CEditMenu>Calendar Edit Menu
	<location id=CalendarEditMenu>&empty;<\location>
<lablist>

\ Time Zone... \<location id=CalendarTimeZonePopupCommand>&empty;<\location>
		Displays a table of time zones, to set for your calendar.

\ Settings... \	<location id=CalendarSettingsPopupCommand>&empty;<\location>
		Displays Calendar properties dialogs.

\ Go to Date... \<location id=CalendarGotoDatePopupCommand>&empty;<\location>
		Prompts for a date to change the calendar display.

\ Find... \	<location id=CalendarFindPopupCommand>&empty;<\location>
		Displays the Find dialog, into which you type information
		about the appointment.  Find displays all appointments
		with matching information.

<\lablist>

<!-- MODULE CViewMenu -->
<s2 id=CViewMenu>Calendar View Menu
	<location id=CalendarViewMenu>&empty;<\location>
<lablist>

\ Day \		<location id=CalendarDay>&empty;<\location>
		Displays the Day View or Appointments for the current day.

\ Week \	<location id=CalendarWeek>&empty;<\location>	
		Displays the Week View or Appointments for the current week.
		
\ Month \	<location id=CalendarMonth>&empty;<\location>
		Displays the Month View or Appointments for the current month.
		
\ Year \	<location id=CalendarYear>&empty;<\location>
		Displays the Year View or Appointments for the current Year.
		
\ ToDo \	<location id=CalendarToDo>&empty;<\location>
		Displays the ToDo list for the current view with options for 
		all items, pending items only, or completed items only.
		

<\lablist>

	
<!-- MODULE CScheduleMenu -->
<s2 id=CScheduleMenu>Calendar Schedule Menu
	<location id=CalendarSchedulMenu>&empty;<\location>

<lablist>

\ Appointment... \<location id=CalendarAppointmentPopupCommand>&empty;<\location>
		Displays the Appointment Editor, used for scheduling appointments.

\ Group Appointment... \<location id=CalendarGroupAppointmentPopupCommand>&empty;<\location>
		Displays the Group Appointment Editor, used for scheduling
		appointments for groups of people.

\ ToDo... \	<location id=CalendarToDoPopupCommand>&empty;<\location>
		Displays the ToDo Editor, used to create of list of things to do.

<\lablist>

<!-- MODULE CBrowseMenu -->
<s2 id=CBrowseMenu>Calendar Browse Menu
	<location id=CalendarBrowseMenu>&empty;<\location>

<lablist>

\ Show Other Calendar... \<location id=CalendarShowOtherCalendarPopupCommand>&empty;<\location>
		Displays the Show Other Calendar dialog, into which you type
		another user's calendar name in the format user@machinename.

\ Compare Calendars... \<location id=CalendarMultiBrowserPopupCommand>&empty;<\location>
		Displays the Compare Calendars dialog, showing users' 
		overlapping appointments in a grid.
		
\ Setup Menu ... \<location id=CalendarSetupMenuPopupCommand>&empty;<\location>
		Displays the Setup Menu dialog, for adding users' calendars for
		comparing.

\ "user@machinename" \<location id=CalendarUsersCalendarsPopupCommand>&empty;<\location>
		List of users' calendars added for comparing using Setup Menu dialog.

<\lablist>

<!-- MODULE CPrintMenu -->
<s2 id=CPrintMenu>Calendar Print Menu
	<location id=CalendarPrintMenu>&empty;<\location>
<!-- Remove Print Menu and create File Menu when UI changes -->

<lablist>

\ Current View \<location id=CalendarCurrentViewPrint>&empty;<\location>

\ Day \		<location id=CalendarDayPrint>&empty;<\location>
		Prints the Day View or list of Appointments for the current day.

\ Week \	<location id=CalendarWeekPrint>&empty;<\location>
		Prints the Week View or list of Appointments for the current week.

\ Month \	<location id=CalendarMonthPrint>&empty;<\location>
		Prints the Month View or list of Appointments for the current month.

\ Year \	<location id=CalendarYearPrint>&empty;<\location>
		Prints the Year View or list of Appointments for the current year.

\ ToDo \	<location id=CalendarToDoPrint>&empty;<\location>
		Prints a list of ToDo items for the current calendar view with
		options for all items, pending items only, or completed items only.

<\lablist>


