	:type			:message
	:message-type		:warning
	:label			"Application Builder"
	:action1-label		"OK"
	:cancel-button		:false
	:help-button		:true
	:default-button		:action1-button
	:help-text		"The project that was opened does not reference\nany modules."
)
:element error_msg
