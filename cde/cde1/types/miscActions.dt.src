ACTION Passwd
{
        LABEL           %|nls-25033-#Change Password#|
        ARG_COUNT       0
        TYPE            COMMAND
        WINDOW_TYPE     NO_STDIO
        EXEC_STRING     dtterm -geometry 60x8 -xrm "*menuBar: False" -e sh -c ' \
                               echo "%|nls-25034-#Changeing#|"; \
                               echo "%|nls-25035-#---------#|"; \
                               passwd'
        DESCRIPTION     %|nls-25036-#The#|
}

