ACTION ExecuteAndSleep
{
	LABEL		%|nls-10086-#Execute#|
        TYPE            COMMAND
        WINDOW_TYPE     PERM_TERMINAL
        EXEC_STRING     sh -c '\
			"%(File)Arg_1"%|nls-10087-#File To Execute:#|"%" \
				%"%|nls-10088-#Options:#|"% \
				%"%|nls-10089-#Arguments:#|"% %(File)Args%; \
			sleep \${_SDT_RUN_BUFFER_SLEEP_SECS:-60}'
}

