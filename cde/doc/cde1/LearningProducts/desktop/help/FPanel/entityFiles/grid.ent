
<!-- ***************  SPECIAL HP GRID FORMATTING  **************** -->

<!entity grid-top-begin PI
"<p left 6 first 6 before 1 after 0 graphic ../graphicsShared/unitylogo.pm gpos right></>
<p left 6 before 0 after 0 graphic ../graphicsShared/grid2.pm ></>
<p left 6 first 6 before 0>">

<!entity grid-title-begin SDATA "<TYPE serif><WEIGHT bold><SIZE 14>">
<!entity grid-title-end SDATA "</SIZE></WEIGHT></TYPE>">

<!entity grid-top-end PI
"</p><p leftindent 6 before 0>
<label><graphic file ../graphicsShared/grid1.pm></label>">

<!entity grid-bottom PI
"</p><p before 1 graphic ../graphicsShared/grid3.pm gpos right></>
">

