OBJS = \
	$(OBJS_ACTION) \
	$(OBJS_BUILDER) \
	$(OBJS_CALC) \
	$(OBJS_CALENDAR) \
	$(OBJS_CUSTOM) \
	$(OBJS_DND) \
	$(OBJS_DATATYPE) \
	$(OBJS_EDITOR) \
	$(OBJS_FILE) \
	$(OBJS_FONT) \
	$(OBJS_HELP) \
	$(OBJS_ICON) \
