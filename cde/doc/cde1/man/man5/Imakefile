#ifdef HPArchitecture  
VENDOR_ARG = -h
#endif

#ifdef RsArchitecture
VENDOR_ARG = -i
#endif

#ifdef SunArchitecture
VENDOR_ARG = -s
#endif

#ifdef USLArchitecture
VENDOR_ARG = -u
#endif


