	DtAction.5 \
	dtactionaction.5

OBJS_BUILDER = \
	dtbuilderaction.5

OBJS_CALC = \
	dtcalcaction.5

OBJS_CALENDAR = \
	csa.5 \
	dtcalendaraction.5

OBJS_CUSTOM = \
	DtComboBox.5 \
	DtMenuButton.5 \
	DtSpinBox.5

OBJS_DATATYPE = \
	DtDts.5

OBJS_DND = \
	DtDnd.5

OBJS_EDITOR = \
