OBJS_ACTIONS = \
	dtactionaction.5 \
	dtappaction.5 \
	dtbuilderaction.5 \
	dtcalcaction.5 \
	dtcalendaraction.5 \
	dtfileaction.5 \
	dthelpaction.5 \
