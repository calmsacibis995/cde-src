	$(OBJS_STYLE) \
	$(OBJS_TERM) \
	$(OBJS_WSM) \
	$(OBJS_MISC)

DT2MAN = ../tools/dt2man

MANROFF = /usr/bin/troff -man


all:: $(OBJS)

actionsall:: $(OBJS_ACTIONS)

actionall:: $(OBJS_ACTION)

builderall:: $(OBJS_BUILDER)

calcall:: $(OBJS_CALC)

calendarall:: $(OBJS_CALENDAR)

customall:: $(OBJS_CUSTOM)

dndall:: $(OBJS_DND)

datatypeall:: $(OBJS_DATATYPE)

editorall:: $(OBJS_EDITOR)

fileall:: $(OBJS_FILE)

fontall:: $(OBJS_FONT)

helpall:: $(OBJS_HELP)

iconall:: $(OBJS_ICON)

initall:: $(OBJS_INIT)

mailall:: $(OBJS_MAIL)

printall:: $(OBJS_PRINT)

sessionall:: $(OBJS_SESSION)

styleall:: $(OBJS_STYLE)

termall:: $(OBJS_TERM)

wsmall:: $(OBJS_WSM)

miscall:: $(OBJS_MISC)


.SUFFIXES: .5xo .5

.5xo.5:
	$(RM) $@
	$(DT2MAN) $(VENDOR_ARG) $? >$@

clean::
	$(RM) $(OBJS)

actionsclean::
	$(RM) $(OBJS_ACTIONS)

actionclean::
	$(RM) $(OBJS_ACTION)

builderclean::
	$(RM) $(OBJS_BUILDER)

