DT2MAN = ../tools/dt2man

MANROFF = /usr/bin/troff -man


all:: $(OBJS)

actionall:: $(OBJS_ACTION)

cmall:: $(OBJS_CM)

customall:: $(OBJS_CUSTOM)

dndall:: $(OBJS_DND)

datatypeall:: $(OBJS_DATATYPE)

editorall:: $(OBJS_EDITOR)

helpall:: $(OBJS_HELP)

initall:: $(OBJS_INIT)

sessionall:: $(OBJS_SESSION)

termall:: $(OBJS_TERM)

wsmall:: $(OBJS_WSM)


.SUFFIXES: .3xo .3

.3xo.3:
	$(RM) $@
	$(DT2MAN) $(VENDOR_ARG) $? >$@

clean::
	$(RM) $(OBJS)

actionclean::
	$(RM) $(OBJS_ACTION)

cmclean::
	$(RM) $(OBJS_CM)

customclean::
	$(RM) $(OBJS_CUSTOM)

dndclean::
	$(RM) $(OBJS_DND)

datatypeclean::
	$(RM) $(OBJS_DATATYPE)

editorclean::
	$(RM) $(OBJS_EDITOR)

helpclean::
	$(RM) $(OBJS_HELP)

initclean::
	$(RM) $(OBJS_INIT)

sessionclean::
	$(RM) $(OBJS_SESSION)

termclean::
	$(RM) $(OBJS_TERM)

wsmclean::
	$(RM) $(OBJS_WSM)


depend::

includes::


actionprint::	actionall
	$(TBL) $(OBJS_ACTION) | $(MANROFF)

cmprint::	cmall
	$(TBL) $(OBJS_CM) | $(MANROFF)

customprint::	customall
	$(TBL) $(OBJS_CUSTOM) | $(MANROFF)

dndprint::	dndall
	$(TBL) $(OBJS_DND) | $(MANROFF)

datatypeprint::	datatypeall
	$(TBL) $(OBJS_DATATYPE) | $(MANROFF)

editorprint:: editorall
	$(TBL) $(OBJS_EDITOR) | $(MANROFF)

helpprint::	helpall
	$(TBL) $(OBJS_HELP) | $(MANROFF)

initprint::	initall
	$(TBL) $(OBJS_INIT) | $(MANROFF)

sessionprint::	sessionall
	$(TBL) $(OBJS_SESSION) | $(MANROFF)

termprint::	termall
	$(TBL) $(OBJS_TERM) | $(MANROFF)

wsmprint::	wsmall
	$(TBL) $(OBJS_WSM) | $(MANROFF)
